   "source": [
    "def in_sort(x):\n",
    "    l = len(x)\n",
    "    for i in range(1, l):\n",
    "        j = i-1\n",
    "        temp = x[i]\n",
    "        while j >=0 and x[j]> temp:\n",
    "            x[j+1] = x[j]\n",
    "            j = j-1\n",
    "        x[j+1] = temp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "bfbc3343",
   "metadata": {},
   "outputs": [],
   "source": [
