   "source": [
    "def bub_sort(x):\n",
    "    l = len(x)\n",
    "    for i in range(l-1):\n",
    "        for j in range(l-1-i):\n",
    "            if x[j] > x[j+1]:\n",
    "                x[j], x[j+1] = x[j+1], x[j]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "abacb231",
   "metadata": {},
   "outputs": [],
   "source": [
